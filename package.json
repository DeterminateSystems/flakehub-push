  "license": "LGPL",
  "bugs": {
    "url": "https://github.com/DeterminateSystems/flakehub-push/issues"
  },
  "homepage": "https://github.com/DeterminateSystems/flakehub-push#readme",
  "dependencies": {
    "@actions/core": "^1.11.1",
    "@actions/exec": "^1.1.1",
    "@actions/github": "^6.0.1",
    "detsys-ts": "github:DeterminateSystems/detsys-ts"
  },
  "devDependencies": {
    "@types/node": "^22.15.18",
    "@vercel/ncc": "^0.38.3",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3"
  },
  "pnpm": {
